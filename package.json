{
  "name": "@alorel/rollup-plugin-threaded-terser",
  "version": "2.0.0",
  "description": "Runs output chunks through terser, each in a separate thread.",
  "main": "index.js",
  "module": "esm/index.js",
  "repository": "https://github.com/Alorel/rollup-plugin-threaded-terser.git",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "author": "Arturas Molcanovas <a.molcanovas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "rollup": "rollup --bundleConfigAsCjs -c",
    "release": "semantic-release",
    "lint": "tslint -s node_modules/custom-tslint-formatters/formatters -t grouped -p .",
    "lint:fix": "yarn run lint --fix",
    "rollup:watch": "yarn run rollup --watch"
  },
  "peerDependencies": {
    "rollup": ">=1.30.0 <4.0.0",
    "terser": ">=4.6.0 <6.0.0"
  },
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.1.3",
    "@alorel-personal/tslint-rules": "^5.0.7",
    "@alorel/rollup-plugin-clean": "^1.0.0",
    "@alorel/rollup-plugin-copy": "^2.0.0",
    "@alorel/rollup-plugin-copy-pkg-json": "^1.0.3",
    "@rollup/plugin-typescript": "^10.0.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@types/node": "^18.11.9",
    "custom-tslint-formatters": "^2.4.0",
    "rollup": "^3.5.0",
    "semantic-release": "^19.0.5",
    "terser": "^5.16.0",
    "tslib": "^2.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/alorel"
  }
